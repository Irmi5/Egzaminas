<Window x:Class="Egzaminas_I.K_.EditEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Egzaminas_I.K_"
        mc:Ignorable="d"
        Title="EditEntry" Height="450" Width="800">
    <Grid>
        <Label Content="Edit Password:" HorizontalAlignment="Left" Margin="50,30,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
        <Label Content="New Password:" HorizontalAlignment="Left" Margin="50,94,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="GeneratePassChange" Content="Generate Password" HorizontalAlignment="Left" Margin="233,123,0,0" VerticalAlignment="Top"/>
        <Button x:Name="AddChange" Content="Change Password" HorizontalAlignment="Left" Margin="50,165,0,0" VerticalAlignment="Top" Width="110" Click="Add_Click"/>
        <Button x:Name="ReturnPageChange" Content="Back to Main Page" HorizontalAlignment="Left" Margin="198,165,0,0" VerticalAlignment="Top" Width="115" Click="ReturnPage_Click"/>
        <PasswordBox x:Name="appPassChange" HorizontalAlignment="Left" Margin="50,120,0,0" VerticalAlignment="Top" Width="168"/>

    </Grid>
</Window>
